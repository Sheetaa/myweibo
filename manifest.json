{
  "manifest_version": 2,

  "name": "MyWeibo",
  "description": "This extension demonstrates personal Weibo content",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "https://*.weibo.com/"
  ]
}
